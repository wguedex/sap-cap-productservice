  Products_0.name,
  Products_0.Price,
  Products_0.Quantity
FROM com_productsrv_Materials_Products AS Products_0;

CREATE VIEW com_productsrv_Sales_SelProducts3 AS SELECT
  ProductReview_1.Rating,
  Products_0.name,
  sum(Products_0.Price) AS TotalPrice
FROM (com_productsrv_Materials_Products AS Products_0 LEFT JOIN com_productsrv_Materials_ProductReview AS ProductReview_1 ON Products_0.name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.name
ORDER BY Rating;

CREATE VIEW CatalogService_Products_texts AS SELECT
  texts_0.locale,
  texts_0.ID,
  texts_0.name,
  texts_0.description
FROM com_productsrv_Materials_Products_texts AS texts_0;

CREATE VIEW localized_com_productsrv_Materials_Products AS SELECT
  L_0.ID,
  L_0.createdAt,
  L_0.createdBy,
  L_0.modifiedAt,
  L_0.modifiedBy,
  coalesce(localized_1.name, L_0.name) AS name,
  coalesce(localized_1.description, L_0.description) AS description,
  L_0.ImageURL,
  L_0.ReleaseDate,
  L_0.DiscontinuedDate,
  L_0.Price,
  L_0.Height,
  L_0.Width,
  L_0.Depth,
  L_0.Quantity,
  L_0.Supplier_ID,
  L_0.UnitOfMeasures_ID,
  L_0.currency_ID,
  L_0.DimensionUnits_ID,
  L_0.Category_ID,
  L_0.priceCondition,
  L_0.priceDetermination
FROM (com_productsrv_Materials_Products AS L_0 LEFT JOIN com_productsrv_Materials_Products_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_productsrv_Materials_Categories AS SELECT
  L_0.ID,
  coalesce(localized_1.Name, L_0.Name) AS Name
FROM (com_productsrv_Materials_Categories AS L_0 LEFT JOIN com_productsrv_Materials_Categories_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_productsrv_Materials_stockAvailability AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description,
  L_0.Product_ID
FROM (com_productsrv_Materials_stockAvailability AS L_0 LEFT JOIN com_productsrv_Materials_stockAvailability_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_productsrv_Materials_Currencies AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (com_productsrv_Materials_Currencies AS L_0 LEFT JOIN com_productsrv_Materials_Currencies_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_productsrv_Materials_UnitOfMeasures AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (com_productsrv_Materials_UnitOfMeasures AS L_0 LEFT JOIN com_productsrv_Materials_UnitOfMeasures_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_productsrv_Materials_DimensionUnits AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (com_productsrv_Materials_DimensionUnits AS L_0 LEFT JOIN com_productsrv_Materials_DimensionUnits_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_productsrv_Sales_Suppliers AS SELECT
  L_0.ID,
  coalesce(localized_1.Name, L_0.Name) AS Name,
  L_0.Street,
  L_0.City,
  L_0.State,
  L_0.PostalCode,
  L_0.Country,
  L_0.Email,
  L_0.Phone,
  L_0.Fax
FROM (com_productsrv_Sales_Suppliers AS L_0 LEFT JOIN com_productsrv_Sales_Suppliers_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_productsrv_Sales_Months AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description,
  coalesce(localized_1.ShortDescription, L_0.ShortDescription) AS ShortDescription
FROM (com_productsrv_Sales_Months AS L_0 LEFT JOIN com_productsrv_Sales_Months_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_productsrv_Materials_ProductReview AS SELECT
  L.ID,
  L.createdAt,
  L.createdBy,
  L.modifiedAt,
  L.modifiedBy,
  L.Name,
  L.Rating,
  L.Comment,
  L.Product_ID
FROM com_productsrv_Materials_ProductReview AS L;

CREATE VIEW localized_com_productsrv_Sales_OrderItems AS SELECT
  L.ID,
  L.Order_ID,
  L.Product_ID,
  L.Quantity
FROM com_productsrv_Sales_OrderItems AS L;

CREATE VIEW localized_com_productsrv_Sales_SalesData AS SELECT
  L.ID,
  L.createdAt,
  L.createdBy,
  L.modifiedAt,
  L.modifiedBy,
  L.DeliveryDate,
