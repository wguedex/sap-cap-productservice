
CREATE TABLE com_productsrv_Materials_Products (
  ID NVARCHAR(36) NOT NULL,
  createdAt TIMESTAMP_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP_TEXT,
  modifiedBy NVARCHAR(255),
  name NVARCHAR(5000) NOT NULL DEFAULT 'NoName',
  description NVARCHAR(5000),
  ImageURL NVARCHAR(5000),
  ReleaseDate TIMESTAMP_TEXT DEFAULT CURRENT_TIMESTAMP,
  DiscontinuedDate TIMESTAMP_TEXT,
  Price DECIMAL(16, 2),
  Height DECIMAL(16, 2),
  Width DECIMAL(16, 2),
  Depth DECIMAL(16, 2),
  Quantity DECIMAL(16, 2),
  Supplier_ID NVARCHAR(36),
  UnitOfMeasures_ID NVARCHAR(2),
  currency_ID NVARCHAR(3),
  DimensionUnits_ID NVARCHAR(2),
  Category_ID NVARCHAR(1),
  priceCondition NVARCHAR(2),
  priceDetermination NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Materials_Categories (
  ID NVARCHAR(1) NOT NULL,
  Name NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Materials_stockAvailability (
  ID INTEGER NOT NULL,
  Description NVARCHAR(5000),
  Product_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Materials_Currencies (
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Materials_UnitOfMeasures (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Materials_DimensionUnits (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Materials_ProductReview (
  ID NVARCHAR(36) NOT NULL,
  createdAt TIMESTAMP_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP_TEXT,
  modifiedBy NVARCHAR(255),
  Name NVARCHAR(5000),
  Rating INTEGER,
  Comment NVARCHAR(5000),
  Product_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Sales_Orders (
  ID NVARCHAR(36) NOT NULL,
  Date DATE_TEXT,
  Customer NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Sales_OrderItems (
  ID NVARCHAR(36) NOT NULL,
  Order_ID NVARCHAR(36),
  Product_ID NVARCHAR(36),
  Quantity INTEGER,
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Sales_Suppliers (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000) NOT NULL DEFAULT 'NoName',
  Street NVARCHAR(5000),
  City NVARCHAR(5000),
  State NVARCHAR(2),
  PostalCode NVARCHAR(5),
  Country NVARCHAR(3),
  Email NVARCHAR(5000),
  Phone NVARCHAR(5000),
  Fax NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Sales_Months (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  ShortDescription NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Sales_SalesData (
  ID NVARCHAR(36) NOT NULL,
  createdAt TIMESTAMP_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP_TEXT,
  modifiedBy NVARCHAR(255),
  DeliveryDate TIMESTAMP_TEXT,
  Revenue DECIMAL(16, 2),
  Product_ID NVARCHAR(36),
  Currency_ID NVARCHAR(3),
  DeliveryMonth_ID NVARCHAR(2),
  PRIMARY KEY(ID)
);

CREATE TABLE com_training_Course (
  ID NVARCHAR(36) NOT NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE com_training_Student (
  ID NVARCHAR(36) NOT NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE com_training_StudentCourse (
  ID NVARCHAR(36) NOT NULL,
  Student_ID NVARCHAR(36),
  Course_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE com_training_car (
  ID NVARCHAR(36) NOT NULL,
  name NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_training_Order (
  clientGender NVARCHAR(5000),
  status INTEGER,
  priority NVARCHAR(5000)
);

CREATE TABLE com_training_Emails (
  email_01 NCLOB,
  email_02 NCLOB,
  email_03 NCLOB
);

CREATE TABLE com_training_Suppliers_01 (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000),
  Address_Street NVARCHAR(5000),
  Address_City NVARCHAR(5000),
  Address_State NVARCHAR(2),
  Address_PostalCode NVARCHAR(5),
  Address_Country NVARCHAR(3),
  Email NVARCHAR(5000),
  Phone NVARCHAR(5000),
  Fax NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_training_Suppliers_02 (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000),
  Address_Street NVARCHAR(5000),
  Address_City NVARCHAR(5000),
  Address_State NVARCHAR(2),
  Address_PostalCode NVARCHAR(5),
  Address_Country NVARCHAR(3),
  Email NVARCHAR(5000),
  Phone NVARCHAR(5000),
  Fax NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_productsrv_Materials_Products_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  name NVARCHAR(5000) NOT NULL DEFAULT 'NoName',
  description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_productsrv_Materials_Categories_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(1) NOT NULL,
  Name NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_productsrv_Materials_stockAvailability_texts (
  locale NVARCHAR(14) NOT NULL,
  ID INTEGER NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_productsrv_Materials_Currencies_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_productsrv_Materials_UnitOfMeasures_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_productsrv_Materials_DimensionUnits_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_productsrv_Sales_Months_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  ShortDescription NVARCHAR(3),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_productsrv_Sales_Suppliers_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000) NOT NULL DEFAULT 'NoName',
  PRIMARY KEY(locale, ID)
);

CREATE VIEW CatalogService_Products AS SELECT
  Products_0.ID,
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.name,
  Products_0.description,
  Products_0.ImageURL,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasures_ID,
  Products_0.currency_ID,
  Products_0.DimensionUnits_ID,
  Category_1.Name AS Category,
  Products_0.priceCondition,
  Products_0.priceDetermination,
  Products_0.UnitOfMeasures_ID AS toUnitOfMeasures_ID,
  Products_0.currency_ID AS toCurrency_ID,
  Products_0.Category_ID AS toCategory_ID,
  Products_0.DimensionUnits_ID AS toDimensionUnit_ID
FROM (com_productsrv_Materials_Products AS Products_0 LEFT JOIN com_productsrv_Materials_Categories AS Category_1 ON Products_0.Category_ID = Category_1.ID);

CREATE VIEW CatalogService_Supplier AS SELECT
  Suppliers_0.ID,
  Suppliers_0.Name,
  Suppliers_0.Email,
  Suppliers_0.Phone,
  Suppliers_0.Fax
FROM com_productsrv_Sales_Suppliers AS Suppliers_0;

CREATE VIEW CatalogService_Reviews AS SELECT
  ProductReview_0.ID,
  ProductReview_0.Name,
  ProductReview_0.Rating,
  ProductReview_0.Comment,
  ProductReview_0.createdAt,
  ProductReview_0.Product_ID AS toProduct_ID
FROM com_productsrv_Materials_ProductReview AS ProductReview_0;

CREATE VIEW CatalogService_SalesData AS SELECT
  SalesData_0.ID,
  SalesData_0.DeliveryDate,
  SalesData_0.Revenue,
  SalesData_0.Currency_ID AS CurrencyKey,
  SalesData_0.DeliveryMonth_ID AS DeliveryMonth_ID,
  SalesData_0.Product_ID AS toProduct_ID
FROM com_productsrv_Sales_SalesData AS SalesData_0;

CREATE VIEW CatalogService_stockAvailability AS SELECT
  stockAvailability_0.ID,
  stockAvailability_0.Description,
  stockAvailability_0.Product_ID AS toProduct_ID
FROM com_productsrv_Materials_stockAvailability AS stockAvailability_0;

CREATE VIEW CatalogService_VH_Categories AS SELECT
  Categories_0.ID AS Code,
  Categories_0.Name AS Text
FROM com_productsrv_Materials_Categories AS Categories_0;

CREATE VIEW CatalogService_VH_Currencies AS SELECT
  Currencies_0.ID AS Code,
  Currencies_0.Description AS Text
FROM com_productsrv_Materials_Currencies AS Currencies_0;

CREATE VIEW CatalogService_VH_UnitOfMeasure AS SELECT
  UnitOfMeasures_0.ID AS Code,
  UnitOfMeasures_0.Description AS Text
FROM com_productsrv_Materials_UnitOfMeasures AS UnitOfMeasures_0;

CREATE VIEW CatalogService_VH_DimensionUnit AS SELECT
  DimensionUnits_0.ID AS Code,
  DimensionUnits_0.Description AS Text
FROM com_productsrv_Materials_DimensionUnits AS DimensionUnits_0;

